<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="TestNG Regression Suite" verbose="3"
	thread-count="3" parallel="tests">


	<test name="seleniumeasylearning">
		<parameter name="url"
			value="https://seleniumeasylearning.blogspot.com/2024/05/implicit-and-explicit-waits.html" />
		<parameter name="browser" value="chrome" />
		<parameter name="headless" value="false" />
		<classes>
			<class name="testngSessions.TestngClass1" />
		</classes>
	</test>


	<test name="OrangeHRM Test">

		<parameter name="url"
			value="https://orangehrm.com/en/30-day-free-trial" />
		<parameter name="browser" value="firefox" />
		<parameter name="headless" value="false" />
		<classes>
			<class name="testngSessions.TestngClass2" />
		</classes>
	</test>


	<test name="OpenCart Test">
		<parameter name="url"
			value="https://naveenautomationlabs.com/opencart/" />
		<parameter name="browser" value="edge" />
		<parameter name="headless" value="false" />

		<classes>
			<class name="testngSessions.TestngClass3" />
		</classes>
	</test>


</suite>